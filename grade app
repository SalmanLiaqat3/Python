score = int(input("Enter your score: "))
if score > 90:
    print("\U0001F601", "A")
elif 90 > score >= 80:
    print("\U0001F602", "B+")
elif 80 > score >= 70:
    print("\U0001F603", "B")
elif 70 > score >= 60:
    print("\U0001F604", "C")
elif 60 > score >= 50:
    print("\U0001F614", "D")
else:
    print("\U0001F608", "F")
